<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.onlinestationerystorev1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".UserRegistration"></activity>
        <activity android:name=".RecurringManagement" />
        <activity android:name=".PaymentMethods" />
        <activity android:name=".PaymentHistory" />
        <activity android:name=".DeliveryDetails" />
        <activity android:name=".OrderSummary" />
        <activity android:name=".ShippingOptions" />
        <activity android:name=".InappPayment" />
        <activity android:name=".CardPayment" />
        <activity android:name=".AddRecurring" />
        <activity android:name=".DiscountPayment" />
        <activity android:name=".PaymentOptions" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>